# Omnify Cloud Connect - AgentKit-First Configuration
# Copy this file to .env and fill in your values

# ========== DATABASE ==========
MONGO_URL=mongodb://localhost:27017
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net
DB_NAME=omnify_cloud

# ========== AGENTKIT ==========
AGENTKIT_API_KEY=your_agentkit_api_key_here
# Get from: https://platform.openai.com/agentkit

# ========== CHATGPT ENTERPRISE ==========
CHATGPT_ENTERPRISE_API_KEY=your_chatgpt_enterprise_key_here
# ChatGPT Enterprise: $30/user/month

# ========== GOHIGHLEVEL ==========
GOHIGHLEVEL_API_KEY=your_gohighlevel_api_key_here
GOHIGHLEVEL_LOCATION_ID=your_location_id_here
# GoHighLevel SaaS Pro: $497/month

# ========== AUTHENTICATION ==========
JWT_SECRET_KEY=your_jwt_secret_key_here_change_in_production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# ========== EXTERNAL APIS ==========
# Google Ads
GOOGLE_ADS_CLIENT_ID=your_google_ads_client_id
GOOGLE_ADS_CLIENT_SECRET=your_google_ads_client_secret
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token

# Meta Ads (Facebook/Instagram)
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret

# LinkedIn Ads
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret

# OpenAI (for custom AI if needed)
OPENAI_API_KEY=your_openai_api_key_here

# ========== STRIPE (BILLING) ==========
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# ========== REDIS (OPTIONAL - CACHING) ==========
REDIS_URL=redis://localhost:6379
# For Upstash: redis://default:password@host:port

# ========== MONITORING ==========
SENTRY_DSN=your_sentry_dsn_here
# Sentry: Free tier or $26/month

# ========== SERVER ==========
PORT=8000
ENVIRONMENT=development
# production, staging, development

# ========== CORS ==========
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000
# Comma-separated list of allowed origins

# ========== LOGGING ==========
LOG_LEVEL=INFO
# DEBUG, INFO, WARNING, ERROR, CRITICAL
